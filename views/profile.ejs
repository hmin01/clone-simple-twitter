<!DOCTYPE html>
<html lang="ko">
<head>
    <meta charset="UTF-8">
    <title>프로</title>
</head>
<body>
<h3>프로</h3>
<form method="POST">
    <table>
        <tr>
            <td>이름 : </td>
            <td><%=name%></td>
        </tr>
        <tr>
            <td>email : </td>
            <td><%=email%></td>
        </tr>
        <tr>
            <td>전화 번호 : </td>
            <td><%=phonenumber%></td>
        </tr>
        <tr>
            <td>성별 : </td>
            <td><%=gender%></td>
        </tr>
        <tr>
            <td>등록 : </td>
            <td><%=registerdate%></td>
        </tr>
        <tr>
            <td>프로필 : </td>
            <td><%=profile%></td>
        </tr>
        <tr>
            <td><input type ="submit" value="수정" id = "update"></td>
        </tr>
    </table>
</form>

<script>
    $("#update").click(function(){
        $.ajax({
            url:'/update',//주
            type: 'POST',
            async: false,
            dataType :'json',
            data: "data",
            success: function (data) {
                console.log('1234')
                alert('success');
            },
            error: function (err) {
                alert(data);
            }
        });
    });
</script>

</body>
</html>