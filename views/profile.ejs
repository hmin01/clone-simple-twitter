<!DOCTYPE html>
<html lang="ko">
<head>
    <meta charset="UTF-8">
    <script src="//code.jquery.com/jquery-1.11.3.min.js"></script>
    <title>프로필</title>
    <a href="/mainhome" height="5" width="5">
        <img src="./bird.png"/>
    </a>
</head>
<body>
<h3>프로필</h3>
<form method="post">
    <table>
        <tr>
            <td>이름 : </td>
            <td><%=name%></td>
        </tr>
        <tr>
            <td>email : </td>
            <td><%=email%></td>
        </tr>
        <tr>
            <td>전화 번호 : </td>
            <td><%=phonenumber%></td>
        </tr>
        <tr>
            <td>성별 : </td>
            <td><%=gender%></td>
        </tr>
        <tr>
            <td>등록 : </td>
            <td><%=registerdate%></td>
        </tr>
        <tr>
            <td>프로필 : </td>
            <td><%=profile%></td>
        </tr>
        <tr>
            <td><input type ="submit" value="수정" id = "update"></td>

            <!--<td><input type ="button" value="삭제" id = "delete"></td>-->
       </tr>
    </table>
</form>

<script>
    $("#update").click(function(){
        $.ajax({
            url:'/update',//주
            type: 'POST',
            async: false,
            dataType :'json',
            data: {
                name : $('#name').val(),
                email : $('#email').val(),
                phonenubmer : $('#phonenumer').val(),
                gender : $('#gender').val(),
                profile : $('#profile').val()
            },
            success: function (data) {
                console.log('1234')
                //location.href="/update";

            },
            error: function (err) {
                alert(data);
            }
        });
    });
</script>

</body>
</html>